<title>{{lang.website_commands_htmltitle}} - LenoxBot</title>
<meta name="description" content="A list of all commands that LenoxBot offers your Discord server...">
<script>
    var currentPageId = "#commands";
</script>
<main id="commands">
    <section class="lb-bg-white">

        <div class="container">
            <div class="row">
                <div class="col-lg-3 my-4">
                    <h3 class="font-weight-light">{{lang.website_commands_modules}} <button class="btn lb-btn-trans-black d-lg-none float-right" id="toggle-menu"><i class="mdi mdi-menu"></i></button></h3>
                    <div id="list-modules" class="list-group menu">
                        <button class="list-group-item list-group-item-action lb-btn-list lb-btn-list-focus" id="command">{{lang.website_commands_all}}</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="administration">Administration</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="moderation">Moderation</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="help">Help</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="music">Music</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="fun">Fun</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="searches">Searches</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="nsfw">NSFW</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="application">Application</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="currency">Currency</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="utility">Utility</button>
                        <button class="list-group-item list-group-item-action lb-btn-list" id="tickets">Tickets</button>
                    </div>
                </div>
                <div class="col-lg-9">
                    <div class="row">
                        <div class="col-12 pr-0 mt-4">
                            <h3 class="font-weight-light">{{lang.website_commands_title}}</h3>
                            <div class="form-group">
                                <input class="form-control lb-input" id="search-input" type="text" placeholder="{{lang.website_commands_search}}">
                            </div>
                        </div>
                        <div class="col-12 lb-commands pr-0 lb-mcsp">
                            <div class="lb-mcsp">
                                <div class="table-responsive">
                                <table id="table" class="table table-hover mb-0">
                                    <thead>
                                        <tr>
                                            <th>{{lang.website_commands_commandname}}</th>
                                            <th>{{lang.website_commands_description}}</th>
                                            <th>{{lang.website_commands_permissions}}</th>
                                            <th>{{lang.website_commands_aliases}}</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each commands}}
                                        <tr class="command {{category}}">
                                            <td><b>{{usage}}</b></td>
                                            <td>{{description}}</td>
                                            <td>{{newuserpermissions}}</td>
                                            <td>{{newaliases}}</td>
                                        </tr>
                                        {{/each}}
                                        <tr id="notfound"  style="display: none;">
                                            <td align="center" colspan="4">{{lang.website_commands_cmdnotfound}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>
<!-- lenoxbot commands JS -->
<script src="js/commands.js"></script>